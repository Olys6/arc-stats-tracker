{
  "expo": {
    "name": "Arc Stats Tracker",
    "slug": "arc-stats-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/arc-stats-tracker-1500x1500.png",
    "scheme": "arcstatstracker",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0a0a0b",
        "foregroundImage": "./assets/images/arc-stats-tracker-1500x1500.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.olys6.arcstatstracker"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/arc-stats-tracker-1500x1500.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/arc-stats-tracker-1500x1500.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0a0a0b"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "497e0ece-7fc3-4029-9938-90cdacc9753b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/497e0ece-7fc3-4029-9938-90cdacc9753b"
    }
  }
}
